<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="match_parent"
	android:icon="@drawable/beer_sm" android:layout_height="match_parent"
	android:weightSum="1">
	<ScrollView android:layout_width="match_parent" android:id="@+id/recents_scrollview" android:layout_height="303dp">
		<LinearLayout android:layout_width="match_parent"
			android:orientation="vertical" android:layout_height="42dp"
			android:id="@+id/recents_layout" android:weightSum="1">
			<ListView android:layout_width="fill_parent"
				android:layout_height="wrap_content" android:id="@+id/android:list" android:layout_weight="8.18"/>
		</LinearLayout>
	</ScrollView>
	<LinearLayout android:layout_width="match_parent" android:id="@+id/linearLayout1" android:layout_height="wrap_content" android:orientation="vertical" android:layout_weight="0.98"></LinearLayout>
	<ImageView android:src="@drawable/beer" android:layout_height="44dp" android:id="@+id/imageView1" android:layout_width="44dp"></ImageView>

</LinearLayout>
